#: model_terms:ir.ui.view,arch_db:fast_food.welcome_article_body
msgid "academy"
msgstr "アカデミー"

#. module: fast_food
#: model_terms:ir.ui.view,arch_db:fast_food.welcome_article_body
msgid "and"
msgstr "と"

#. module: fast_food
#: model_terms:ir.ui.view,arch_db:fast_food.welcome_article_body
msgid "documentation"
msgstr "ドキュメント"

