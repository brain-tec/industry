#. module: bike_shop
#: model:mail.template,body_html:bike_shop.mail_template_warranty_claim
msgid ""
"<div style=\"margin: 0px; padding: 0px;\" data-oe-version=\"1.0\"> \n"
"            <p style=\"box-sizing:border-box;margin: 0px; padding: 0px; font-size: 12px;\"> \n"
"            Dear <span style=\"font-weight:bold;\" t-out=\"object.partner_id.name or ''\"/>,<br/>\n"
"            <br/> \n"
"            We are submitting a warranty claim for a customer.\n"
"            </p><ul>\n"
"                <t t-if=\"object.properties and object.properties.get('order_line_id')\">\n"
"                    <t t-set=\"order_line\" t-value=\"env['sale.order.line'].browse(object.properties['order_line_id'])\"/>\n"
"                    <li>\n"
"                        Customer Name: <t t-out=\"order_line.order_id.partner_id.display_name or 'N/A'\"/>\n"
"                    </li>\n"
"                </t>\n"
"                <t t-if=\"object.properties and object.properties.get('task_id')\">\n"
"                    <t t-set=\"task\" t-value=\"env['project.task'].browse(object.properties['task_id'])\"/>\n"
"                    <t t-if=\"task.task_properties and task.task_properties.get('serial_number_id')\">\n"
"                        <t t-set=\"serial\" t-value=\"env['stock.lot'].browse(task.task_properties['serial_number_id'])\"/>\n"
"\n"
"                        <t t-set=\"move_lines\" t-value=\"env['stock.move.line'].search([('lot_id', '=', serial.id)])\"/>\n"
"                        <t t-set=\"sale_date\" t-value=\"False\"/>\n"
"                        <t t-foreach=\"move_lines\" t-as=\"ml\">\n"
"                            <t t-if=\"ml.move_id.sale_line_id\">\n"
"                                <t t-set=\"sale_date\" t-value=\"ml.move_id.sale_line_id.order_id.date_order\"/>\n"
"                                <t t-break=\"True\"/>\n"
"                            </t>\n"
"                        </t>\n"
"                        <li>\n"
"                            Serial Number: <t t-out=\"serial.name or 'N/A'\"/>\n"
"                        </li>\n"
"                        <li t-if=\"sale_date\">\n"
"                            Purchase Date: <t t-out=\"format_date(sale_date) if sale_date else 'N/A'\"/>\n"
"                        </li>\n"
"                    </t>\n"
"                </t>\n"
"            </ul>\n"
"            Issue Description: <t t-out=\"object.description\"/>\n"
"            <br/>\n"
"            Please find attached photos of the defect.<br/>\n"
"            <br/>\n"
"            Thank you, <br/>\n"
"            <t t-out=\"object.user_id.display_name\"/> <br/>\n"
"            <t t-out=\"object.company_id.name\"/> \n"
"            </div> "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\" data-oe-version=\"1.0\"> \n"
"            <p style=\"box-sizing:border-box;margin: 0px; padding: 0px; font-"
"size: 12px;\"> \n"
"            Dear <span style=\"font-weight:bold;\" t-out=\""
"object.partner_id.name or ''\"/>,<br/>\n"
"            <br/> \n"
"            Wir reichen einen Garantieanspruch für einen Kunden ein.\n"
"            </p><ul>\n"
"                <t t-if="
"\"object.properties and object.properties.get('order_line_id')\">\n"
"                    <t t-set=\"order_line\" t-value="
"\"env['sale.order.line'].browse(object.properties['order_line_id'])\"/>\n"
"                    <li>\n"
"                        Name des Kunden: <t t-out="
"\"order_line.order_id.partner_id.display_name or 'N/A'\"/>\n"
"                    </li>\n"
"                </t>\n"
"                <t t-if="
"\"object.properties and object.properties.get('task_id')\">\n"
"                    <t t-set=\"task\" t-value="
"\"env['project.task'].browse(object.properties['task_id'])\"/>\n"
"                    <t t-if="
"\"task.task_properties and task.task_properties.get('serial_number_id')\">\n"
"                        <t t-set=\"serial\" t-value="
"\"env['stock.lot'].browse(task.task_properties['serial_number_id'])\"/>\n"
"\n"
"                        <t t-set=\"move_lines\" t-value="
"\"env['stock.move.line'].search([('lot_id', '=', serial.id)])\"/>\n"
"                        <t t-set=\"sale_date\" t-value=\"False\"/>\n"
"                        <t t-foreach=\"move_lines\" t-as=\"ml\">\n"
"                            <t t-if=\"ml.move_id.sale_line_id\">\n"
"                                <t t-set=\"sale_date\" t-value="
"\"ml.move_id.sale_line_id.order_id.date_order\"/>\n"
"                                <t t-break=\"True\"/>\n"
"                            </t>\n"
"                        </t>\n"
"                        <li>\n"
"                            Seriennummer: <t t-out=\"serial.name or 'N/A'\"/>"
"\n"
"                        </li>\n"
"                        <li t-if=\"sale_date\">\n"
"                            Kaufdatum: <t t-out="
"\"format_date(sale_date) if sale_date else 'N/A'\"/>\n"
"                        </li>\n"
"                    </t>\n"
"                </t>\n"
"            </ul>\n"
"            Problembeschreibung: <t t-out=\"object.description\"/>\n"
"            <br/>\n"
"            Anbei finden Sie Fotos des Mangels.<br/>\n"
"            <br/>\n"
"            Vielen Dank! <br/>\n"
"            <t t-out=\"object.user_id.display_name\"/> <br/>\n"
"            <t t-out=\"object.company_id.name\"/> \n"
"            </div> "

