<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <function model="calendar.attendee" name="write">
    <value model="calendar.attendee" eval="obj().search([
      ('partner_id', 'in', [ref('res_partner_34'), ref('res_partner_20'), ref('res_partner_35'), ref('res_partner_38')]),
      ('event_id', '=', ref('calendar_event_30')),
    ]).ids"/>
    <value eval="{'x_attending': True}"/>
  </function>
  <function model="calendar.attendee" name="write">
    <value model="calendar.attendee" eval="obj().search([
      ('partner_id', '=', ref('base.partner_admin')),
      ('event_id', '=', ref('calendar_event_30')),
    ]).ids"/>
    <value eval="{'state': 'accepted'}"/>
  </function>
</odoo>
