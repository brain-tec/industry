#. module: summer_camps
#: model_terms:ir.ui.view,arch_db:summer_camps.welcome_article_body
msgid ""
"<strong>\n"
"                        <span class=\"h2-fs\">Tips for Success</span>\n"
"                    </strong>"
msgstr ""
"<strong>\n"
"                        <span class=\"h2-fs\">成功のヒント</span>\n"
